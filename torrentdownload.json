{
  "name": "torrentdownload",
  "language": ["en"],
  "domains": ["torrentdownload.info"],
  "base_url": "https://www.torrentdownload.info",
  "search_url_format_episode": {
    "string_format": "/search?q=%s+S%02dE%02d",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search?q=%s+%04d",
    "replacement": [
      "title",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/search?q=%s",
    "replacement": [
      "title"
    ]
  },
  "more_pages_indicator": "Next",
  "next_page_path" : "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "table.table2 tr td:eq(0) div.tt-name a[href]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "table.table2 ",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "table.table3.torrentinfo tbody tr:eq(0) td:eq(1)",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "tr:eq(4) td span.bigtosa a[href]",
  "magnet_path_attr": "href",
  "seeders_path": "table.table3.torrentinfo tbody tr:eq(4) td:eq(1) span",
  "seeders_path_attr": "",
  "filesize_path": "table.table3.torrentinfo tbody tr:eq(5) td:eq(1)",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
